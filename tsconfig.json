{
  "compilerOptions": {
    "target": "es2016",
    "lib": [
      "es2016"
    ],
    "esModuleInterop": true,
    "noEmit": true,
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true,
    "strict": true,
    "types": [
      "cypress"
    ]
  },
  "files": [
    "cypress/**"
  ]
}
{
  "compilerOptions": {
    "target": "es2016",
    "lib": [
      "DOM",
      "ES6",
    ],
    "declaration": true,
    "esModuleInterop": true,
    "noEmit": true,
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true,
    "strict": true,
    "types": [
      "cypress",
      "node"
    ]
  },
  "include": [
    "src/**/*.ts",
    "src/index.d.ts",
  ]
}